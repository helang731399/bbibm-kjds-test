<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bbibm.center.order.mapper.BbibmKjdsOrderMapper">

  <resultMap id="bbibmKjdsOrderMap" type="com.bbibm.center.order.entity.BbibmKjdsOrder">
                  <id property="estimatedShippingFee" column="estimated_shipping_fee"/>
                        <result property="paymentMethod" column="payment_method"/>
                        <result property="messageToSeller" column="message_to_seller"/>
                        <result property="shippingCarrier" column="shipping_carrier"/>
                        <result property="currency" column="currency"/>
                        <result property="createTime" column="create_time"/>
                        <result property="payTime" column="pay_time"/>
                        <result property="daysToShip" column="days_to_ship"/>
                        <result property="shipByDate" column="ship_by_date"/>
                        <result property="trackingNo" column="tracking_no"/>
                        <result property="orderStatus" column="order_status"/>
                        <result property="updateTime" column="update_time"/>
                        <result property="escrowAmount" column="escrow_amount"/>
                        <result property="goodsToDeclare" column="goods_to_declare"/>
                        <result property="totalAmount" column="total_amount"/>
                        <result property="serviceCode" column="service_code"/>
                        <result property="country" column="country"/>
                        <result property="ordersn" column="ordersn"/>
                        <result property="buyerUsername" column="buyer_username"/>
            </resultMap>

      <update id="UpdateStatus">
            update bbibm_kjds_order
            set order_status= #{status}
            where ordersn = #{Ordersn}
     </update>

      <select id="select">


      </select>
</mapper>
