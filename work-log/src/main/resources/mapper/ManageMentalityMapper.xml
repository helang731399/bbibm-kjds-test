<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bbibm.log.mapper.ManageMentalityMapper">

  <resultMap id="manageMentalityMap" type="com.bbibm.log.entity.ManageMentality">
                  <id property="logId" column="log_ID"/>
                        <result property="earnest" column="earnest"/>
                        <result property="quick" column="quick"/>
                        <result property="honest" column="honest"/>
                        <result property="commitment" column="commitment"/>
                        <result property="optimistic" column="optimistic"/>
                        <result property="confident" column="confident"/>
                        <result property="love" column="love"/>
                        <result property="noReason" column="no_Reason"/>
            </resultMap>

    <select id="selectNoID" resultType="com.bbibm.log.repvo.ManageMentalityVo">
        select earnest,quick,honest,commitment,optimistic,confident,love,no_Reason
        from manage_mentality
        where log_ID = #{log_ID}
    </select>
</mapper>
